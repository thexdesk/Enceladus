    "lint:css": "stylelint \"src/css/**/*.pcss\" \"src/css/**/*.sss\"",
    "lint:css:fix": "stylelint --fix \"src/css/**/*.pcss\" \"src/css/**/*.sss\"",
    "serve": "http-server dist -p 5000",
    "postinstall": "cd babel-plugin-custom-element && npm i && npm run build"
